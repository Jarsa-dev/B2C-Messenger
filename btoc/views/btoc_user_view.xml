            <field name="name">btoc.user</field>
            <field name="model">btoc.user</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
            	<sheet>
            		<field name="name" position="before">
						<field name="telegram_id"/>
